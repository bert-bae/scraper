{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "allowJs": true,
    "outDir": "ts-build/",
    "typeRoots": ["./typings", "node_modules/@types"]
  },
  "include": ["src/*"],
  "exclude": ["node_modules", ".npm"]
}
